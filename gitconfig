[core]
  editor = code --wait

[push]
  default = current

[alias]
  a = add .
  b = branch
  c = checkout
  cb = checkout -b
  d = diff
  p = push
  s = status -sb
  ca = commit --amend
  cm = commit --message
  r = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbi = rebase --interactive
